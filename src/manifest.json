{
    "manifest_version": 2,
    "name": "M Radio",
    "version": "0.0.1",
    "icons": {
        "124": "images/mradio-logo.png"
    },
    "browser_action": {
        "default_icon": {
            "90": "images/mradio-logo.png",
            "124": "images/mradio-logo.png"
        },
        "default_title": "M Radio",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "*://mradio.fr/*"
    ]
}
